<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>Typer brugt i kurser og foredrag</Title>
    <Company>mcronberg</Company>
    <RepositoryUrl>https://github.com/mcronberg/MCronberg.Kursus</RepositoryUrl>
    <Description>Simple typer (terning, spillekort, spillebunke mv) brugt i frobindelse med kurser og foredrag</Description>
    <Copyright></Copyright>
    <PackageProjectUrl>https://github.com/mcronberg/MCronberg.Kursus</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

</Project>
